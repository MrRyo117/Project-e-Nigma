<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="666.0" prefWidth="666.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projectenigma.SampleController">

    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <AnchorPane fx:id="AnchorPane" minWidth="-Infinity" prefHeight="578.0" prefWidth="1266.0" />
   <HBox alignment="CENTER" prefHeight="0.0" prefWidth="1266.0">
      <children>
         <Button cache="true" mnemonicParsing="false" onAction="#DibujoLed" text="Led">
            <HBox.margin>
               <Insets right="5.0" />
            </HBox.margin></Button>
         <Button mnemonicParsing="false" onAction="#Cables" text="Cable">
            <HBox.margin>
               <Insets right="5.0" />
            </HBox.margin></Button>
         <Button mnemonicParsing="false" onAction="#DibujoSwitch" text="Switch">
            <HBox.margin>
               <Insets right="5.0" />
            </HBox.margin></Button>
         <Button mnemonicParsing="false" onAction="#DibujoChip" text="Chip">
            <HBox.margin>
               <Insets right="5.0" />
            </HBox.margin></Button>
         <Button mnemonicParsing="false" text="Resistencia">
            <HBox.margin>
               <Insets right="5.0" />
            </HBox.margin>
         </Button>
         <Button fx:id="Boton_encendido" mnemonicParsing="false" onAction="#Cambio_estado_bateria" text="Apagar" textFill="RED">
            <HBox.margin>
               <Insets />
            </HBox.margin></Button>
         <Button mnemonicParsing="false" onAction="#Borrar_pieza" text="Borrar">
            <HBox.margin>
               <Insets right="5.0" />
            </HBox.margin></Button>
      </children>
   </HBox>

</VBox>
